00013 int8_t\par
00014 extract_file (FILE *fptr, directory_record *dr, {\cf17 const} {\cf18 char} *path)\par
00015 \{\par
00016   {\cf20 /*}\par
00017 {\cf20    *  the `file_identifier` terminates with a `;` character followed by the}\par
00018 {\cf20    *  file ID number in ASCII coded decimal (`1`).}\par
00019 {\cf20    *  See: https://wiki.osdev.org/ISO_9660#Directories}\par
00020 {\cf20    */}\par
00021   {\cf18 char} *actual_filename = strtok (dr->file_identifier, ({\cf17 const} {\cf18 char} *){\cf22 ";"});\par
00022   {\cf19 if} (actual_filename == NULL)\par
00023     \{\par
00024       {\cf20 /*}\par
00025 {\cf20        *  Just use the default/existing filename.}\par
00026 {\cf20        *  It'll be incorrect, but probably not worth stoping execution over.}\par
00027 {\cf20        *  Users could just manually remove the `;1` part; the data itself}\par
00028 {\cf20        * should be fine.}\par
00029 {\cf20        */}\par
00030       actual_filename = dr->file_identifier;\par
00031     \}\par
00032 \par
00033   printf ({\cf22 "Extracting file: %s\\n"}, actual_filename);\par
00034 \par
00035   {\cf20 // +1 for the null terminator, +1 for `/` between dir and filename}\par
00036   {\cf18 size_t} filename_length = strlen (path) + strlen (actual_filename) + 2;\par
00037 \par
00038   {\cf18 char} *output_filename = ({\cf18 char} *)calloc (filename_length, {\cf17 sizeof} ({\cf18 char}));\par
00039   {\cf19 if} (output_filename == NULL)\par
00040     \{\par
00041       perror ({\cf22 "ERROR: unable to calloc `output_filename`."});\par
00042       {\cf19 return} -1;\par
00043     \}\par
00044 \par
00045   strcpy (output_filename, path);\par
00046   strcat (output_filename, &OP_PATH_SEPARATOR);\par
00047   strcat (output_filename, actual_filename);\par
00048 \par
00049   FILE *output_file = fopen (output_filename, {\cf22 "wb"});\par
00050   {\cf19 if} (output_file == NULL)\par
00051     \{\par
00052       perror ({\cf22 "ERROR: unable to open output file."});\par
00053       free (output_filename);\par
00054       {\cf19 return} -1;\par
00055     \}\par
00056 \par
00057   {\cf20 // `j` must be in hex, otherwise `data_length` can be treated as an int value}\par
00058   {\cf19 for} (uint32_t j = 0x0; j < dr->data_length; j++)\par
00059     \{\par
00060       uint8_t {\cf18 byte} = read_single_uint8 (fptr);\par
00061       fwrite (&{\cf18 byte}, {\cf17 sizeof} (uint8_t), 1, output_file);\par
00062     \}\par
00063 \par
00064   fclose (output_file);\par
00065   free (output_filename);\par
00066 \par
00067   {\cf19 return} 0;\par
00068 \}\par
00069 \par
00070 {\cf20 /*}\par
00071 {\cf20  *  extract_directory}\par
00072 {\cf20  *}\par
00073 {\cf20  *  TODO: add documentation}\par
00074 {\cf20  */}\par
00075 int8_t\par
00076 extract_directory (FILE *fptr, {\cf17 const} uint16_t BLOCK_SIZE, {\cf17 const} {\cf18 char} *path)\par
00077 \{\par
00078   {\cf20 // 0xF00000 == 15 MiB}\par
00079   {\cf17 const} uint32_t DEBUG_FILE_SIZE_LIMIT = 0xF00000;\par
00080 \par
00081   directory dir;\par
00082   create_directory (&dir);\par
00083   process_directory (fptr, &dir);\par
00084 \par
00085   printf ({\cf22 "Extracting directory: %s\\n"}, path);\par
00086 \par
00087   {\cf19 for} ({\cf18 size_t} i = 0x0; i < dir.current_record; i++)\par
00088     \{\par
00089       directory_record curr_file = dir.records[i];\par
00090 \par
00091       {\cf19 if} (curr_file.file_flags.subdirectory)\par
00092         \{\par
00093           {\cf19 continue};\par
00094         \}\par
00095       {\cf19 else} {\cf19 if} (OP_DEBUG_MODE && curr_file.data_length > DEBUG_FILE_SIZE_LIMIT)\par
00096         \{\par
00097           printf ({\cf22 "[DEBUG_MODE] Skipping file, %s.\\n"},\par
00098                   curr_file.file_identifier);\par
00099           {\cf19 continue};\par
00100         \}\par
00101 \par
00102       fseek (fptr, curr_file.location_of_extent * BLOCK_SIZE, SEEK_SET);\par
00103 \par
00104       {\cf19 if} (extract_file (fptr, &curr_file, path) != 0)\par
00105         \{\par
00106           destroy_directory (&dir);\par
00107           {\cf19 return} -1;\par
00108         \}\par
00109     \}\par
00110 \par
00111   destroy_directory (&dir);\par
00112   {\cf19 return} 0;\par
00113 \}\par
00114 \par
00115 {\cf20 /*}\par
00116 {\cf20  *  create_directories_and_extract_data_from_path_file}\par
00117 {\cf20  *}\par
00118 {\cf20  *  TODO: add documentation}\par
00119 {\cf20  */}\par
00120 {\cf18 void}\par
00121 create_directories_and_extract_data_from_path_file (FILE *fptr,\par
00122                                                     uint16_t BLOCK_SIZE,\par
00123                                                     path_table *pt)\par
00124 \{\par
00125   {\cf19 for} ({\cf18 size_t} i = pt->current_entry - 1; i > 0; --i)\par
00126     \{\par
00127       path_table_entry curr_dir = pt->entries[i];\par
00128       path_table_entry target_dir = curr_dir;\par
00129 \par
00130       {\cf20 // supports 10 levels of directories which is probably overkill.}\par
00131       {\cf17 const} uint32_t PATH_MAX_LEN\par
00132           = ((curr_dir.directory_identifier_length + 1) * 10)\par
00133             + (strlen (OUTPUT_DIR) + 1) + (strlen (OP_CURRENT_DISK_NAME) + 1)\par
00134             + 1;\par
00135       {\cf18 char} *path = calloc (PATH_MAX_LEN, {\cf17 sizeof} ({\cf18 char}));\par
00136       {\cf19 if} (path == NULL)\par
00137         \{\par
00138           perror ({\cf22 "ERROR: unable to calloc path string"});\par
00139           destroy_path_table (pt);\par
00140           exit (1);\par
00141         \}\par
00142 \par
00143       strcat (path, curr_dir.directory_identifier);\par
00144 \par
00145       {\cf19 do}\par
00146         \{\par
00147           uint16_t index\par
00148               = change_endianness_uint16 (curr_dir.parent_directory_number);\par
00149           {\cf20 // parent_directory_number is 1-based}\par
00150           curr_dir = pt->entries[index - 1];\par
00151 \par
00152           prepend_path_string (path,\par
00153                                ({\cf17 const} {\cf18 char} *)curr_dir.directory_identifier);\par
00154         \}\par
00155       {\cf19 while} (curr_dir.parent_directory_number > 0x0100);\par
00156 \par
00157       create_output_directory (path);\par
00158 \par
00159       fseek (fptr, BLOCK_SIZE * target_dir.location_of_extent, SEEK_SET);\par
00160 \par
00161       extract_directory (fptr, BLOCK_SIZE, path);\par
00162 \par
00163       free (path);\par
00164     \}\par
00165 \}\par
}
